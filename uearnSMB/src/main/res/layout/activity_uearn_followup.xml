                            android:layout_height="wrap_content"
                            android:background="#0000"
                            android:text="Accept"
                            android:textColor="@color/black"
                            android:textSize="16dp"
                            android:textStyle="bold" />
                    </LinearLayout>
                </LinearLayout>

